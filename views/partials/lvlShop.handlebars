
<div class="Shop">
  <heading class="shop-open">

<h1>One Stop!</h1>
  </heading>
<div class="store-front">
<h2>Shopping for Goods</h2>

<ul class="shop">
{{#each items}}
	<li class="shopItem" value="{{this.id}}" id="item{{this.id}}">
	<p> {{this.id}}. {{this.item_name}}
	</li>
{{/each}}
</ul>
</div>

<div class="backpack">
<h2>these items are yours</h2>
<br>
<ul class="ulBackpack">
</ul>
</div>

<script type="text/javascript" src="/assets/js/shop.js"></script>
